INFILE=$1

if [ -z ${INFILE} ]; then
    mysql -h auroramysql001-instance-1.czzdit7hfndz.us-east-2.rds.amazonaws.com -P 3306  -u admin -p`cat ~/.pw` --ssl_mode=DISABLED
else
    mysql -h auroramysql001-instance-1.czzdit7hfndz.us-east-2.rds.amazonaws.com -P 3306  -u admin -p`cat ~/.pw` --ssl_mode=DISABLED < ${INFILE}
fi

