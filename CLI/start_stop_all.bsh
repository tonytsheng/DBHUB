ACTIONIN=$1
ACTION=$(echo "$ACTIONIN" | tr '[:upper:]' '[:lower:]')-db-instance
#echo ${ACTION}

for ins in `/usr/local/bin/aws rds describe-db-instances | jq ' .DBInstances[] | .DBInstanceIdentifier' | sed s/\"//g `
do
    echo $ins
    /usr/local/bin/aws rds ${ACTION} --db-instance-identifier $ins --output text
done

