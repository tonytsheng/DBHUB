aws rds describe-db-instances --profile dba | jq ' .DBInstances[]|  .DBInstanceIdentifier, .DBInstanceStatus'
aws rds delete-db-instance  --db-instance-identifier ttsora14 --skip-final-snapshot  --profile dba
aws rds create-db-instance-read-replica --db-instance-identifier ttsora16 --availability-zone "us-east-2a" --source-db-instance-identifier ttsora10 --profile dba --db-instance-class db.r5.large

